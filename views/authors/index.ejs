<h2>Search Authros</h2>
<form>
    <label>Name</label>
    <input type="text" name="name" value=<%= searchOptions.name%>>
    <button type="submit">Search</button>
</form>
<%if(authors && authors.length > 0 ){%>

    <%authors.forEach(author=>{%>
        <div><%-author.name%></div>
    <%})%>

<%}else{%>
    <p>No Found Authors.</p>
<%}%>
